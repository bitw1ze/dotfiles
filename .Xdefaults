! urxvt stuff
URxvt.font: xft:DejaVu Sans Mono-9
URxvt.letterSpace: -1
URxvt.urgentOnBell: True
URxvt.foreground: #ffffff
URxvt.background: #000000
URxvt.scrollBar: False
URxvt.transparent: True
URxvt.shading: 15
URxvt.releasestring: rxvt
URxvt.fading: 20
!URxvt.fadeColor: #000000
URxvt.skipScroll: True

! Xfont stuff
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintlight
Xft.rgba: rgb

! XURxvt.perl-ext-common: ...,keyboard-select
! URxvt.keysym.M-Escape: perl:keyboard-select:activate
! ft.dpi: 96
